{
    "name": "byng/pimcore-doctrine-migrations-library",
    "description": "A library to add integration for Doctrine migrations in Pimcore.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "doctrine/migrations": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Byng\\Pimcore\\DoctrineMigrations\\": "src/"
        }
    },
    "extra": {
        "class": "Byng\\Pimcore\\DoctrineMigrations\\Install"
    }
}
